<%# CONVERTED FROM SLIM - NEEDS MANUAL ERB CONVERSION %>
= admin_page_header t('.sponsors'), t('.manager_sponsors')
.container-fluid
  .fade-in
    .row
      .col-md-12
        = admin_box do
          = admin_box_body class: 'no-padding' do
            table.table.table-striped
              thead
                tr
                  th.table__id-column #
                  th = t('.name')
                  th = t('.level')
                  th = t('.order')
                  th.col-md-3 = t('.actions')
              tbody
                = render @sponsors
